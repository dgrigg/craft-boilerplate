{
  "name": "dgrigg/craft-3-boilerplate",
  "description": "Craft 3 boilerplate setup, includes basic js/css/esbuild setup",
  "version": "1.3.3",
  "authors": [
    {
      "name": "Derrick Grigg",
      "email": "derrick@dgrigg.com"
    }
  ],
  "support": {
    "docs": "https://github.com/dgrigg/craft-3-boilerplate/blob/master/README.md",
    "issues": "https://github.com/dgrigg/craft-3-boilerplate/issues"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "craftcms/cms": "3.7.27.2",
    "craftcms/redactor": "2.10.3",
    "mmikkel/retcon": "2.3.0",
    "nystudio107/craft-seomatic": "3.4.26",
    "nystudio107/craft-twigpack": "1.2.15",
    "putyourlightson/craft-blitz": "3.11.1",
    "sebastianlenz/linkfield": "^1.0",
    "topshelfcraft/wordsmith": "3.3.0.1",
    "verbb/navigation": "1.4.24",
    "verbb/super-table": "2.6.8",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
