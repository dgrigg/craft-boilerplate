{
  "name": "dgrigg/craft-3-boilerplate",
  "description": "Craft 3 boilerplate setup, includes basic js/css/webpack setup",
  "version": "1.0.8",
  "authors": [
    {
      "name": "Derrick Grigg",
      "email": "derrick@dgrigg.com"
    }
  ],
  "support": {
    "docs": "https://github.com/dgrigg/craft-3-boilerplate/blob/master/README.md",
    "issues": "https://github.com/dgrigg/craft-3-boilerplate/issues"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "craftcms/cms": "^3.2.6",
    "vlucas/phpdotenv": "^2.4.0",
    "verbb/super-table": "^2.1.16",
    "verbb/navigation": "^1.1.11",
    "craftcms/redactor": "^1.0",
    "sebastianlenz/linkfield": "^1.0.17",
    "nystudio107/craft-seomatic": "^3.1.46",
    "spicyweb/craft-neo": "^2.2.8",
    "dgrigg/craft-migration-assistant": "^3.1.1"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
